{
	"config": {
        "notDefined": "notDefined",
        "defaultRouter": "main",
        "routers": {
            "main": ["notDefined", {
                "query": ["notDefined", {
                    "gremlin": "query_gremlin",
                    "mql": "query_mql"
                }],
                "shape": ["notDefined", {
                    "gremlin": "shape_gremlin",
                    "mql": "shape_mql"
                }],
                "write": ["write_json_nquad", {
                    "file": ["notDefined", {
                        "nquad": "write_file_nquad"
                    }]
                }],
                "delete": "delete",
                "auth": "auth",
                "token": ["notDefined", {
                    ":token": "token"
                }]
            }]
        }
    },
	"flow": {
        "auth": {
            "d": [
                ":flow_auth/token.create",
                [":http_server/send", {
                    "code": 200,
                    "end": true,
                    "send": "token"
                }]
            ],
            "r": "http_server/http_auth_err"
        },
        "token": {
            "d": [
                [":flow_tools/transform", {
                    "token": "{params.token}"
                }], 
                ":flow_auth/token.validate",
                [":http_server/send", {
                    "code": 200,
                    "end": true,
                    "send": "content"
                }]
            ],
            "r": "http_server/http_auth_err"
        },
		"notDefined": {
			"d": [
                [":http_server/send", {
                    "code": 404,
                    "end": true
                }]
            ]
		},
        "query_gremlin": {
            "d": [
                ":flow_api/query.gremlin",
                [":http_server/send", {
                    "code": 200,
                    "end": true,
                    "send": "body"
                }]
            ]
        }
	},
	"name": "http_router",
	"module": "flow-router",
	"roles": {
		"*": true
	}
}
