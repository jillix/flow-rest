{
	"config": {
        "notDefined": "notDefined",
        "routes": ["notDefined", {
            "query": ["notDefined", {
                "gremlin": "query_gremlin",
                "mql": "query_mql"
            }],
            "shape": ["notDefined", {
                "gremlin": "shape_gremlin",
                "mql": "shape_mql"
            }],
            "write": ["write_json_nquad", {
                "file": ["notDefined", {
                    "nquad": "write_file_nquad"
                }]
            }],
            "delete": "delete"
        }]
    },
	"flow": {
		"notDefined": {
			"d": [
                [":http_server/send", {
                    "code": 404,
                    "end": true,
                    "send": "url"
                }]
            ]
		},
        "query_gremlin": {
            "d": [
                ":flow_api/query.gremlin",
                [":http_server/send", {
                    "code": 200,
                    "end": true,
                    "send": "result"
                }]
            ]
        }
	},
	"name": "http_router",
	"module": "flow-router",
	"roles": {
		"*": true
	}
}
